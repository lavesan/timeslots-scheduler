generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

// Exemplo b√°sico de modelo
model TimeSlot {
    id        String   @id @default(uuid())
    startTime DateTime
    endTime   DateTime
    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt
}
